    "exponential-backoff": "^3.1.3",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "luxon": "^3.7.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  }
