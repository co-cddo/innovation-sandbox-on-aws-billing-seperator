{
  "name": "isb-billing-separator",
  "version": "1.0.0",
  "private": true,
  "description": "ISB Billing Separator - Temporary quarantine solution for billing attribution",
  "type": "module",
  "main": "dist/bin/billing-separator.js",
  "scripts": {
    "build": "npx tsc -p tsconfig.build.json",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --maxWorkers=1",
    "lint": "eslint . --ext .ts",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all",
    "validate": "npm run lint && npm run test && npm run build",
    "synth": "cdk synth",
    "diff": "cdk diff"
  },
  "keywords": [
    "aws",
    "cdk",
    "isb",
    "billing",
    "sandbox"
  ],
  "author": "",
