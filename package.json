    "@aws-lambda-powertools/tracer": "^2.31.0",
    "@aws-sdk/client-organizations": "^3.995.0",
    "@aws-sdk/credential-providers": "^3.995.0",
    "@aws-sdk/lib-dynamodb": "^3.995.0",
    "@eslint/js": "^10.0.1",
    "@types/aws-lambda": "^8.10.160",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.54.0",
    "aws-cdk-lib": "^2.240.0",
    "constructs": "^10.4.5",
    "esbuild": "^0.27.3",
